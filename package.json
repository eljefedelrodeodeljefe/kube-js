{
  "name": "kubernetes-client",
  "version": "0.1.0",
  "description": "Simplified, lighweight Kubernetes API client with some useful extensions.",
  "main": "lib/index.js",
  "scripts": {
    "check-coverage": "istanbul check-coverage",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha && istanbul report cobertura",
    "eslint": "eslint -c .eslintrc lib/ test/",
    "jsdoc": "jsdoc -d doc lib",
    "jscs": "standard lib/ test/",
    "lint": "standard lib/ test/",
    "release": "standard-version --tag-prefix=''",
    "test": "mocha",
    "test-integration": "TESTING=int mocha -t ${TIMEOUT:-30000} test"
  },
  "repository": "godaddy/kubernetes-client",
  "keywords": [
    "kubernetes",
    "kubectl",
    "containers"
  ],
  "author": "Robert Lindstaedt",
  "license": "MIT",
  "engines": {
    "node": ">=4.2.0"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "js-yaml": "^3.7.0",
    "request": "^2.79.0",
    "standard": "^10.0.2",
    "steed": "^1.1.3"
  },
  "devDependencies": {
    "assume": "^1.4.1",
    "eslint": "^3.19.0",
    "godaddy-style": "^3.1.5",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "sinon": "1.17.6",
    "standard-version": "^4.0.0"
  }
}
