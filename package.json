{
  "name": "kube-js",
  "version": "0.1.2",
  "description": "Simplified, lightweight Kubernetes API client with some useful extensions.",
  "main": "lib/index.js",
  "scripts": {
    "check-coverage": "istanbul check-coverage",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha && istanbul report cobertura",
    "eslint": "eslint -c .eslintrc lib/ test/",
    "jsdoc": "jsdoc -d doc lib",
    "jscs": "standard lib/ test/",
    "lint": "standard lib/ test/",
    "test": "mocha",
    "test-integration": "TESTING=int mocha -t ${TIMEOUT:-30000} test",
    "postversion": "TAG=$(echo $(git describe --tags $(git rev-list --tags --max-count=1))) && git tag -a $TAG $TAG -f -m \"$(git log `git describe --tags --abbrev=0 HEAD^`..HEAD --oneline)\" && git push && git push --tags"
  },
  "repository": "eljefeldelrodeodeljefe/kubernetes-client",
  "keywords": [
    "kubernetes",
    "kubectl",
    "containers"
  ],
  "author": "Robert Jefe Lindstaedt <robert.lindstaedt@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=4.2.0"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "js-yaml": "^3.7.0",
    "request": "^2.79.0",
    "standard": "^10.0.2",
    "steed": "^1.1.3"
  },
  "devDependencies": {
    "assume": "^1.4.1",
    "eslint": "^3.19.0",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "sinon": "1.17.6",
    "standard-version": "^4.0.0"
  }
}
